<Application x:Class="Ui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Ui"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Core;component/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Core;component/Styles/Colors.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Core;component/Styles/ComboBoxes.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Core;component/Styles/ListBoxes.xaml"/>
                <ResourceDictionary Source="/Styles/Fonts.xaml"/>
                <ResourceDictionary Source="/Controls/SpinningRing.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
