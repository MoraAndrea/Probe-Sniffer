<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ProbeSniffer.Styles">

    <Color x:Key="GreenColor">#ff6afff3</Color>
    <SolidColorBrush x:Key="GreenColorBrush" Color="{StaticResource GreenColor}"/>
    
    <Color x:Key="DarkGreenColor">#ff6afff3</Color>
    <SolidColorBrush x:Key="DarkGreenColorBrush" Color="{StaticResource DarkGreenColor}"/>

    <Color x:Key="VioletColor">#ffbf71ff</Color>
    <SolidColorBrush x:Key="VioletColorBrush" Color="{StaticResource VioletColor}"/>

    <Color x:Key="GreyColor">#ff9193a8</Color>
    <SolidColorBrush x:Key="GreyColorBrush" Color="{StaticResource GreyColor}"/>

    <Color x:Key="YellowColor">#fffff38e</Color>
    <SolidColorBrush x:Key="YellowColorBrush" Color="{StaticResource YellowColor}"/>

    <Color x:Key="AzureColor">#ff1cbfff</Color>
    <SolidColorBrush x:Key="AzureColorBrush" Color="{StaticResource AzureColor}"/>

    <Color x:Key="BackgroundColor">#ff141828</Color>
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{StaticResource BackgroundColor}"/>

</ResourceDictionary>